@model JwtTest.Models.OrderModel
@{
    ViewData["Title"] = $"Информация о заказе {Model.Id}";
}
<div>
    <h4>Заказ</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Description)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Content)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DeadLine)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Price)
        </dd>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.OrderStatus)
		</dd>
    </dl>
</div>
